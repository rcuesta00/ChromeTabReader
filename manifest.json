{
    "manifest_version": 2,
    "name": "Coding training",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [    
                "https://www.youtube.com/*",
                "http://www.youtube.com/*"
            ],
            "js": ["content.js"]
        }
    ],
    "permissions": ["tabs"],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "defaul_title": "show tab inspector"
    }
}